#!/bin/bash
# AUTOMATICALLY GENERATED
# DO NOT EDIT THIS FILE DIRECTLY, USE /post_push.erb and /update

set -Eeuo pipefail

# Parse image name for repo name
tag_start=$(expr index "$IMAGE_NAME" :)
repo_name=${IMAGE_NAME:0:tag_start-1}

# Tag and push image for each additional tag
for tag in {2-lua52compat-alpine,2-lua52compat-alpine3.8,2.0-lua52compat-alpine,2.0-lua52compat-alpine3.8,2.0.5-lua52compat-alpine,2.0.5-lua52compat-alpine3.8,lua52compat-alpine}; do
  docker tag "$IMAGE_NAME" "${repo_name}:${tag}"
  docker push "${repo_name}:${tag}"
done
