      exitcode=1
      echo "Unexpected Error"
      echo "$output"
      echo
    fi

    printf "2.0-centos8: "
    output=$(docker run -v $(pwd)/test.sh:/test.sh -e "LANG=luajit" -e "LUAROCKS=false" --rm nickblah/luajit:2.0-centos8 /test.sh 2>&1 || true)
    if [[ "$output" =~ "Lua Tests: Error" ]]; then
      exitcode=1
      echo "Error"
      echo "$output"
      echo
    elif [[ "$output" =~ "Lua Tests: OK" ]]; then
      echo "OK"
